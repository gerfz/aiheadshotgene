{
  "expo": {
    "name": "Act",
    "slug": "ai-portrait-studio",
    "version": "1.6.2",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "dark",
    "scheme": "aiportrait",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#0F172A"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.aiportrait.studio",
      "buildNumber": "1"
    },
    "android": {
      "icon": "./assets/icon.png",
      "package": "com.aiportrait.studio",
      "versionCode": 102,
      "permissions": [
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.CAMERA",
        "android.permission.READ_MEDIA_IMAGES",
        "android.permission.READ_MEDIA_VISUAL_USER_SELECTED",
        "com.android.vending.BILLING",
        "android.permission.READ_MEDIA_VIDEO",
        "android.permission.READ_MEDIA_AUDIO",
        "android.permission.RECORD_AUDIO"
      ],
      "blockedPermissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "android.permission.READ_MEDIA_AUDIO",
        "android.permission.RECORD_AUDIO"
      ],
      "enableProguardInReleaseBuilds": true,
      "enableShrinkResourcesInReleaseBuilds": true
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      "./plugins/withTikTokSDK.js",
      [
        "react-native-appsflyer",
        {}
      ],
      [
        "expo-build-properties",
        {
          "android": {
            "kotlinVersion": "2.0.21",
            "compileSdkVersion": 35,
            "targetSdkVersion": 35,
            "minSdkVersion": 24,
            "usesCleartextTraffic": false,
            "enableProguardInReleaseBuilds": true,
            "enableShrinkResourcesInReleaseBuilds": true,
            "enableMinifyInReleaseBuilds": true
          }
        }
      ],
      "expo-router",
      [
        "expo-media-library",
        {
          "photosPermission": "Allow Act to access your photos to save generated portraits.",
          "savePhotosPermission": "Allow Act to save photos to your gallery.",
          "isAccessMediaLocationEnabled": false,
          "legacyExternalStorage": false
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "Allow Act to access your photos to upload for portrait generation.",
          "cameraPermission": "Allow Act to take photos for portrait generation."
        }
      ]
    ],
    "extra": {
      "router": {},
      "eas": {
        "projectId": "ca5a8852-6029-4793-be6e-83375af5cb60"
      }
    },
    "owner": "gerfzz"
  }
}
