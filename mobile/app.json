{
  "expo": {
    "name": "AI Portrait Studio",
    "slug": "ai-portrait-studio",
    "version": "1.0.5",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "dark",
    "scheme": "aiportrait",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#0F172A"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.aiportrait.studio",
      "buildNumber": "1"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#0F172A"
      },
      "package": "com.aiportrait.studio",
      "versionCode": 6,
      "permissions": [
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.CAMERA",
        "android.permission.READ_MEDIA_IMAGES",
        "android.permission.READ_MEDIA_VISUAL_USER_SELECTED",
        "android.permission.ACCESS_MEDIA_LOCATION",
        "com.android.vending.BILLING"
      ],
      "blockedPermissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      [
        "expo-build-properties",
        {
          "android": {
            "kotlinVersion": "2.0.21",
            "compileSdkVersion": 35,
            "targetSdkVersion": 35,
            "minSdkVersion": 24,
            "usesCleartextTraffic": false
          }
        }
      ],
      "expo-router",
      [
        "expo-media-library",
        {
          "photosPermission": "Allow AI Portrait Studio to access your photos to save generated portraits.",
          "savePhotosPermission": "Allow AI Portrait Studio to save photos to your gallery.",
          "isAccessMediaLocationEnabled": true
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "Allow AI Portrait Studio to access your photos to upload for portrait generation.",
          "cameraPermission": "Allow AI Portrait Studio to take photos for portrait generation."
        }
      ]
    ],
    "extra": {
      "router": {},
      "eas": {
        "projectId": "636e8aad-a6d3-48e3-85fc-cb2c62fcb402"
      }
    },
    "owner": "gerfz"
  }
}
